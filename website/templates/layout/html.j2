<!DOCTYPE html>
<html>

    <head>

        <title>BASEBOT</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

        {% block embed %}
        <meta name="twitter:card" content="{% block embed_card %}summary{% endblock embed_card %}" />
        <meta name="twitter:title" content="{% block embed_title %}BASEBOT{% endblock embed_title %}" />
        <meta name="twitter:description" content="{% block embed_description %}BASEBOT DESCRIPTION{% endblock embed_description %}" />
        <meta name="twitter:image" content="{% block embed_image %}{% endblock embed_image %}" />
        {% endblock embed%}

        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
        <link rel="stylesheet" href="{{ static('css/navbar.css') }}" />
        {% block stylesheets %}{% endblock stylesheets %}

        {# TODO add OpenGraph tags #}

    </head>
    <body class="bg-light">

        {%- block navbar -%}{%- endblock navbar -%}

        {%- block content -%}{%- endblock content -%}

        <!-- Add the Bootstrap JS stuff -->
        {# <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script> #}
        <script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        {% block scripts %}{% endblock scripts %}

    </body>

</html>
